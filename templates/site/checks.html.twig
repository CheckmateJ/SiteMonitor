{% extends 'base.html.twig' %}

{% block body %}

    {% include '/nav/tabs.html.twig' with {'id': id} %}
        <table class="table">
            <thead>
            <tr>
                <th>Domain Name</th>
                <th>Status</th>
                <th>Created at</th>
            </tr>
            </thead>
            <tbody>
            {% for site in siteCheck %}
                <tr>
                    <td>{{ site.site }}</td>
                    <td>{{ site.statusCode }}</td>
                    <td>{{ site.createdAt|date }}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
{% endblock %}